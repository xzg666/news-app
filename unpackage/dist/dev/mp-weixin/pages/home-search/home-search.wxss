page {
  width: 100%;
  height: 100%;
  display: flex;
  background-color: #f5f5f5;
}
.home {
  width: 100%;
}
.home .home-list {
  padding: 10px;
  background-color: #fff;
}
.home .home-list .label-box .label-header {
  display: flex;
  justify-content: space-between;
  margin-bottom: 15px;
}
.home .home-list .label-box .label-header .label-title {
  color: #f07373;
}
.home .home-list .label-box .label-header .label-clear {
  color: #30b33a;
  font-weight: bold;
}
.home .home-list .label-box .label-content {
  display: flex;
  flex-wrap: wrap;
  padding-right: 20px;
  box-sizing: border-box;
}
.home .home-list .label-box .label-content .label-content-item {
  padding: 2px 8px;
  border: #333 1px solid;
  border-radius: 8px;
  margin: 0 10px 10px 0;
}
.noData {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 50px 0;
  color: #333;
}

